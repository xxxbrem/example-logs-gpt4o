-- Description: Calculate the percentage of CoinJoin transactions in 2021 for each month.
SELECT 
    monthly."block_timestamp_month", 
    (coinjoin."coinjoin_count"::FLOAT / monthly."transaction_count") * 100 AS "coinjoin_transaction_percentage"
FROM 
    (SELECT "block_timestamp_month", COUNT(*) AS "transaction_count"
     FROM CRYPTO.CRYPTO_BITCOIN.TRANSACTIONS
     WHERE "block_timestamp_month" BETWEEN '2021-01-01' AND '2021-12-31'
     GROUP BY "block_timestamp_month") monthly
JOIN 
    (SELECT "block_timestamp_month", COUNT(*) AS "coinjoin_count"
     FROM CRYPTO.CRYPTO_BITCOIN.TRANSACTIONS
     WHERE "block_timestamp_month" BETWEEN '2021-01-01' AND '2021-12-31'
       AND "output_count" > 2 
       AND "output_value" <= "input_value"
     GROUP BY "block_timestamp_month") coinjoin
ON monthly."block_timestamp_month" = coinjoin."block_timestamp_month"
ORDER BY monthly."block_timestamp_month"
LIMIT 20;
