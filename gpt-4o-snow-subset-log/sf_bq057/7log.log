     WHERE "block_timestamp_month" BETWEEN '2021-01-01' AND '2021-12-31'
     GROUP BY "block_timestamp_month") monthly
JOIN 
    (SELECT "block_timestamp_month", COUNT(*) AS "coinjoin_count", SUM("output_value") AS "coinjoin_volume", SUM("input_count") AS "total_inputs", SUM("output_count") AS "total_outputs"
     FROM CRYPTO.CRYPTO_BITCOIN.TRANSACTIONS
     WHERE "block_timestamp_month" BETWEEN '2021-01-01' AND '2021-12-31'
       AND "output_count" > 2 
       AND "output_value" <= "input_value"
     GROUP BY "block_timestamp_month") coinjoin
ON monthly."block_timestamp_month" = coinjoin."block_timestamp_month"
ORDER BY monthly."block_timestamp_month";
