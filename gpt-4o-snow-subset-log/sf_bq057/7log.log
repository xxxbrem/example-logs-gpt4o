FROM 
    CRYPTO.CRYPTO_BITCOIN.TRANSACTIONS t
WHERE 
    t."block_timestamp_month" BETWEEN '2021-01-01' AND '2021-12-31' 
    AND t."output_count" > 2 
    AND t."output_value" <= t."input_value"
GROUP BY 
    t."block_timestamp_month"
ORDER BY 
    t."block_timestamp_month"
