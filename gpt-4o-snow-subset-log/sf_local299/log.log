    FROM (
        SELECT 
            "customer_id", 
            "txn_date", 
            SUM(CASE WHEN "txn_type" = 'deposit' THEN "txn_amount" ELSE -1 * "txn_amount" END) AS "running_balance"
        FROM "BANK_SALES_TRADING"."BANK_SALES_TRADING"."CUSTOMER_TRANSACTIONS"
        GROUP BY "customer_id", "txn_date"
    ) SubQ
)
