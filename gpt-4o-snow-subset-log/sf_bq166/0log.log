```sql
-- Description: Calculate the frequency percentages of each copy number category across TCGA-KIRC cases.
WITH max_copy_numbers AS (
    SELECT c."case_barcode",
           c."chromosome",
           b."cytoband_name",
           MAX(c."copy_number") AS "max_copy_number",
           CASE
               WHEN MAX(c."copy_number") > 3 THEN 'amplifications'
               WHEN MAX(c."copy_number") = 3 THEN 'gains'
               WHEN MAX(c."copy_number") = 2 THEN 'normal_diploid'
               WHEN MAX(c."copy_number") = 1 THEN 'heterozygous_deletions'
               WHEN MAX(c."copy_number") = 0 THEN 'homozygous_deletions'
               ELSE 'others'
           END AS "copy_number_category"
    FROM TCGA_MITELMAN.TCGA_VERSIONED.COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23 c
    INNER JOIN TCGA_MITELMAN.PROD.CYTOBANDS_HG38 b
    ON c."chromosome" = b."chromosome"
       AND c."start_pos" <= b."hg38_stop"
       AND c."end_pos" >= b."hg38_start"
    WHERE c."project_short_name" = 'TCGA-KIRC'
    GROUP BY c."case_barcode", c."chromosome", b."cytoband_name"
),
category_counts AS (
    SELECT "copy_number_category",
           COUNT(DISTINCT "case_barcode") AS "num_cases"
    FROM max_copy_numbers
    GROUP BY "copy_number_category"
),
total_cases AS (
    SELECT COUNT(DISTINCT "case_barcode") AS "total_cases"
    FROM TCGA_MITELMAN.TCGA_VERSIONED.COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23
    WHERE "project_short_name" = 'TCGA-KIRC'
)
SELECT cc."copy_number_category",
       cc."num_cases",
       t."total_cases",
       (cc."num_cases" * 100.0 / t."total_cases") AS "frequency_percentage"
FROM category_counts cc, total_cases t
ORDER BY cc."copy_number_category"
LIMIT 20;
