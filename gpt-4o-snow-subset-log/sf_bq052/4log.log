INNER JOIN PATENTSVIEW.PATENTSVIEW.CPC_CURRENT cpc
  ON p."id" = cpc."patent_id"
LEFT JOIN (
  SELECT a."patent_id", COUNT(c."uuid") AS "backward_citation_count"
  FROM PATENTSVIEW.PATENTSVIEW.APPLICATION a
  INNER JOIN PATENTSVIEW.PATENTSVIEW.USPATENTCITATION c
    ON c."citation_id" = a."patent_id"
  WHERE TRY_TO_DATE(c."date", 'YYYY-MM-DD') IS NOT NULL 
    AND TRY_TO_DATE(a."date", 'YYYY-MM-DD') IS NOT NULL 
    AND DATEDIFF(DAY, TRY_TO_DATE(c."date", 'YYYY-MM-DD'), TRY_TO_DATE(a."date", 'YYYY-MM-DD')) BETWEEN 1 AND 30
  GROUP BY a."patent_id"
) bc ON p."id" = bc."patent_id"
LEFT JOIN (
  SELECT a."patent_id", COUNT(c."uuid") AS "forward_citation_count"
  FROM PATENTSVIEW.PATENTSVIEW.APPLICATION a
  INNER JOIN PATENTSVIEW.PATENTSVIEW.USPATENTCITATION c
