WITH RECURSIVE
/* 1.  German patents granted in December 2016 */
dec16_patents AS (
    SELECT  p."publication_number",
            TO_DATE(p."filing_date"::VARCHAR,'YYYYMMDD')  AS filing_dt,
            TO_DATE(p."grant_date" ::VARCHAR,'YYYYMMDD')  AS grant_dt,
            p."cpc"
    FROM    PATENTS.PATENTS.PUBLICATIONS p
    WHERE   p."country_code" = 'DE'
      AND   p."grant_date" BETWEEN 20161201 AND 20161231
),
/* 2.  Extract every CPC code and reduce it to the level-4 symbol (e.g. H04L, G06F) */
cpc_codes AS (
    SELECT  d."publication_number",
            YEAR(d.filing_dt)                                            AS filing_year,
            REGEXP_SUBSTR(f.value:"code"::STRING, '^[A-Z]\\d{2}[A-Z]')   AS cpc_lvl4
    FROM    dec16_patents d,
            LATERAL FLATTEN(input => d."cpc") f
    WHERE   f.value:"code" IS NOT NULL
),
/* 3.  Yearly filing counts per CPC level-4 group                         */
yearly_counts AS (
    SELECT  cpc_lvl4  AS grp,
            filing_year AS yr,
            COUNT(*)    AS filings
    FROM    cpc_codes
    WHERE   cpc_lvl4 IS NOT NULL
    GROUP BY grp, yr
),
/* 4.  Order the years for each group                                      */
ordered AS (
    SELECT  grp,
            yr,
            filings,
            ROW_NUMBER() OVER (PARTITION BY grp ORDER BY yr) AS rn
    FROM    yearly_counts
),
/* 5.  Recursive calculation of EMA with α = 0.1                           */
ema_calc AS (
    /* anchor */
    SELECT  grp,
            yr,
            filings,
            filings * 0.1                     AS ema,
            rn
    FROM    ordered
    WHERE   rn = 1
    UNION ALL
    /* recursion */
    SELECT  o.grp,
            o.yr,
            o.filings,
            o.filings * 0.1 + e.ema * 0.9     AS ema,
            o.rn
    FROM    ordered o
    JOIN    ema_calc e
           ON o.grp = e.grp
          AND o.rn  = e.rn + 1
),
/* 6.  Pick the year with the highest EMA for each CPC group               */
max_ema AS (
    SELECT  grp,
            yr               AS max_ema_year,
            ema,
            ROW_NUMBER() OVER (PARTITION BY grp ORDER BY ema DESC, yr) AS rk
    FROM    ema_calc
)
/* 7.  Final output with CPC definitions (level 4)                         */
SELECT  d."titleFull"   AS cpc_title,
        m.grp           AS cpc_group,
        m.max_ema_year
FROM    max_ema               m
JOIN    PATENTS.PATENTS.CPC_DEFINITION d
          ON d."symbol" = m.grp
WHERE   d."level" = 4
  AND   m.rk = 1
ORDER BY d."titleFull" NULLS LAST;
