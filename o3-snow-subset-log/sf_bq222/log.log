    find the CPC level-4 groups that reached their highest
    exponential moving average (α = 0.1) of yearly filings       */
WITH RECURSIVE
/* 1. German publications dated Dec-2016 ------------------------*/
dec16_pub AS (
    SELECT  p."publication_number",
            TRY_TO_DATE(p."filing_date"::STRING , 'YYYYMMDD') AS filing_dt,
            p."cpc"
    FROM    PATENTS.PATENTS.PUBLICATIONS  p
    WHERE   p."country_code" = 'DE'
      AND   p."publication_date" BETWEEN 20161201 AND 20161231
),
/* 2. Explode CPC codes and keep level-4 symbol -----------------*/
cpc_lvl4 AS (
    SELECT  d."publication_number",
            YEAR(d.filing_dt)                                          AS filing_year,
            REGEXP_SUBSTR(f.value:"code"::STRING , '^[A-Z]\\d{2}[A-Z]') AS grp
    FROM    dec16_pub d,
            LATERAL FLATTEN(input => d."cpc") f
    WHERE   f.value:"code" IS NOT NULL
      AND   d.filing_dt            IS NOT NULL
      AND   REGEXP_SUBSTR(f.value:"code"::STRING , '^[A-Z]\\d{2}[A-Z]') IS NOT NULL
),
/* 3. Yearly filing counts per CPC group ------------------------*/
yearly AS (
    SELECT  grp,
            filing_year AS yr,
            COUNT(*)    AS filings
    FROM    cpc_lvl4
    GROUP BY grp, yr
),
/* 4. Order rows to prepare for recursion -----------------------*/
ordered AS (
    SELECT  grp,
            yr,
            filings,
            ROW_NUMBER() OVER (PARTITION BY grp ORDER BY yr) AS rn
    FROM    yearly
),
/* 5. Recursive EMA computation (α = 0.1) -----------------------*/
ema AS (
    /* anchor */
    SELECT  grp, yr, filings,
            filings * 0.1                         AS ema,
            rn
    FROM    ordered
    WHERE   rn = 1
    UNION ALL
    /* step */
    SELECT  o.grp,
            o.yr,
            o.filings,
            o.filings * 0.1 + e.ema * 0.9         AS ema,
            o.rn
    FROM    ordered o
    JOIN    ema  e
      ON    o.grp = e.grp
     AND    o.rn  = e.rn + 1
),
/* 6. Take the year with the highest EMA per group --------------*/
max_ema AS (
