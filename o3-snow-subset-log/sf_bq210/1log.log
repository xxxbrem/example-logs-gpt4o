Query:
--Description: Optional refinement – treat patents with no English-language claims as “no-claim-word”
-- Counts US-B2 patents (2008-2018) whose English claims never contain “claim”.
SELECT COUNT(*) AS "us_b2_no_claim_word_incl_no_en_text"
FROM (
        SELECT  p."publication_number",
                MAX(
                    CASE
                        WHEN f.value IS NOT NULL
                             AND f.value:"language"::STRING = 'en'
                             AND f.value:"text"::STRING ILIKE '%claim%'
                        THEN 1
                        ELSE 0
                    END
                ) AS has_claim_word
        FROM    PATENTS.PATENTS.PUBLICATIONS  p
             ,  LATERAL FLATTEN(input => p."claims_localized", OUTER => TRUE) f   -- keeps rows even if array null
        WHERE   p."country_code" = 'US'
          AND   p."kind_code"    = 'B2'
          AND   p."grant_date"  BETWEEN 20080101 AND 20181231
        GROUP BY p."publication_number"
     ) t
WHERE has_claim_word = 0
LIMIT 20
