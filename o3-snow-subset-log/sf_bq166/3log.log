--10. Description:  Compute frequency (%) of every CNV-class per cytoband (sorted) for TCGA-KIRC
WITH per_band AS (
  SELECT seg."project_short_name",
         seg."chromosome",
         cb."cytoband_name",
         MAX(seg."copy_number") AS "max_copy"
  FROM "TCGA_MITELMAN"."TCGA_VERSIONED"."COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23" seg
  JOIN "TCGA_MITELMAN"."PROD"."CYTOBANDS_HG38"                     cb
    ON seg."chromosome" = cb."chromosome"
   AND seg."start_pos"  < cb."hg38_stop"
   AND seg."end_pos"    > cb."hg38_start"
  WHERE seg."project_short_name" = 'TCGA-KIRC'
  GROUP BY seg."project_short_name", seg."chromosome", cb."cytoband_name"
),
classified AS (
  SELECT "project_short_name",
         "chromosome",
         "cytoband_name",
         CASE
           WHEN "max_copy" > 3 THEN 'Amplification'
           WHEN "max_copy" = 3 THEN 'Gain'
           WHEN "max_copy" = 2 THEN 'Normal'
           WHEN "max_copy" = 1 THEN 'Heterozygous Deletion'
           WHEN "max_copy" = 0 THEN 'Homozygous Deletion'
         END AS "cnv_class"
  FROM per_band
),
totals AS (
  SELECT COUNT(DISTINCT "project_short_name") AS "total_cases"
  FROM classified
)
SELECT c."chromosome",
       c."cytoband_name",
       c."cnv_class",
       COUNT(DISTINCT c."project_short_name") * 100.0 / t."total_cases" AS "frequency_pct"
FROM classified c, totals t
GROUP BY c."chromosome", c."cytoband_name", c."cnv_class", t."total_cases"
ORDER BY c."chromosome", c."cytoband_name"
LIMIT 20;
