```sql
--Description: 7)  Classify those per-cytoband maxima into CNV categories
SELECT t."project_short_name",
       t."chromosome",
       t."cytoband_name",
       t."max_copy_number",
       CASE
            WHEN t."max_copy_number"  > 3 THEN 'Amplification'
            WHEN t."max_copy_number"  = 3 THEN 'Gain'
            WHEN t."max_copy_number"  = 2 THEN 'Normal'
            WHEN t."max_copy_number"  = 1 THEN 'Heterozygous Deletion'
            WHEN t."max_copy_number"  = 0 THEN 'Homozygous Deletion'
            ELSE 'Other'
       END                                   AS "cnv_class"
FROM (
      SELECT s."project_short_name",
             s."chromosome",
             b."cytoband_name",
             MAX(s."copy_number") AS "max_copy_number"
      FROM "TCGA_MITELMAN"."TCGA_VERSIONED"."COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23" s
      JOIN "TCGA_MITELMAN"."PROD"."CYTOBANDS_HG38"                                    b
        ON s."chromosome" = b."chromosome"
       AND LEAST(s."end_pos", b."hg38_stop") > GREATEST(s."start_pos", b."hg38_start")
      WHERE s."project_short_name" = 'TCGA-KIRC'
      GROUP BY s."project_short_name", s."chromosome", b."cytoband_name"
) t
LIMIT 20;
