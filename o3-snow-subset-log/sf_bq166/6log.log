```sql
--Description: 10)  Convert the counts to percentages of total KIRC cases and sort by chromosome & cytoband
WITH total AS (
     SELECT COUNT(DISTINCT "project_short_name") AS "n_cases"
     FROM "TCGA_MITELMAN"."TCGA_VERSIONED"."COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23"
     WHERE "project_short_name" = 'TCGA-KIRC'
), per_band AS (
     SELECT t."chromosome",
            t."cytoband_name",
            t."cnv_class",
            COUNT(DISTINCT t."project_short_name") AS "num_cases"
     FROM (
           SELECT s."project_short_name",
                  s."chromosome",
                  b."cytoband_name",
                  MAX(s."copy_number") AS "max_copy_number",
                  CASE
                        WHEN MAX(s."copy_number")  > 3 THEN 'Amplification'
                        WHEN MAX(s."copy_number")  = 3 THEN 'Gain'
                        WHEN MAX(s."copy_number")  = 2 THEN 'Normal'
                        WHEN MAX(s."copy_number")  = 1 THEN 'Heterozygous Deletion'
                        WHEN MAX(s."copy_number")  = 0 THEN 'Homozygous Deletion'
                        ELSE 'Other'
                  END AS "cnv_class"
           FROM "TCGA_MITELMAN"."TCGA_VERSIONED"."COPY_NUMBER_SEGMENT_ALLELIC_HG38_GDC_R23" s
           JOIN "TCGA_MITELMAN"."PROD"."CYTOBANDS_HG38"                                   b
             ON s."chromosome" = b."chromosome"
            AND LEAST(s."end_pos", b."hg38_stop") > GREATEST(s."start_pos", b."hg38_start")
           WHERE s."project_short_name" = 'TCGA-KIRC'
           GROUP BY s."project_short_name", s."chromosome", b."cytoband_name"
     ) t
     GROUP BY t."chromosome", t."cytoband_name", t."cnv_class"
)
SELECT p."chromosome",
       p."cytoband_name",
       p."cnv_class",
       ROUND(100.0 * p."num_cases" / t."n_cases", 2) AS "frequency_pct"
FROM per_band  p
CROSS JOIN total t
ORDER BY p."chromosome", p."cytoband_name", p."cnv_class"
LIMIT 20;
```
