--Description: Aggregate motorcycle-collision counts and summed fatalities for the two helmet groups.
WITH helmet_flags AS (
  SELECT DISTINCT p."case_id",
         CASE 
           WHEN (
             UPPER(p."party_safety_equipment_1") ILIKE '%HELMET%NOT%WORN%'
             OR UPPER(p."party_safety_equipment_2") ILIKE '%HELMET%NOT%WORN%'
           ) THEN 'no_helmet'
           WHEN (
             UPPER(p."party_safety_equipment_1") ILIKE '%HELMET%WORN%'
             OR UPPER(p."party_safety_equipment_2") ILIKE '%HELMET%WORN%'
           ) THEN 'helmet_worn'
           ELSE 'unknown'
         END AS "helmet_status"
  FROM "CALIFORNIA_TRAFFIC_COLLISION"."CALIFORNIA_TRAFFIC_COLLISION"."PARTIES" p
  WHERE p."statewide_vehicle_type" ILIKE '%motorcycle%'
)
SELECT hf."helmet_status",
       COUNT(DISTINCT hf."case_id")                                   AS "motorcycle_collisions",
       SUM(COALESCE(c."motorcyclist_killed_count",0))                 AS "motorcyclist_fatalities"
FROM helmet_flags hf
JOIN "CALIFORNIA_TRAFFIC_COLLISION"."CALIFORNIA_TRAFFIC_COLLISION"."COLLISIONS" c
  ON hf."case_id" = c."case_id"
WHERE hf."helmet_status" IN ('helmet_worn','no_helmet')
GROUP BY hf."helmet_status"
LIMIT 20;
