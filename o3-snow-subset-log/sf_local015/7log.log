--Description: Aggregate motorcycle-collision counts and summed fatalities for
--the two helmet groups, using “HELMET … USED” and “HELMET … NOT USED”.
WITH helmet_flags AS (
  SELECT DISTINCT p."case_id",
         CASE
           WHEN (
              UPPER(p."party_safety_equipment_1") ILIKE '%HELMET%NOT%USED%'
           OR UPPER(p."party_safety_equipment_2") ILIKE '%HELMET%NOT%USED%'
           ) THEN 'no_helmet'
           WHEN (
              UPPER(p."party_safety_equipment_1") ILIKE '%HELMET%USED%'
           OR UPPER(p."party_safety_equipment_2") ILIKE '%HELMET%USED%'
           ) THEN 'helmet_worn'
           ELSE 'unknown'
         END AS "helmet_status"
