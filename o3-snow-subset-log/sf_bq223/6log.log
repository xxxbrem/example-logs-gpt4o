--Description: Bring everything together – citing assignee, primary CPC subclass
--              title, and number of citations of DENSO patents (still capped).
WITH denso_pubs AS (
    SELECT DISTINCT t."publication_number"
    FROM PATENTS.PATENTS.PUBLICATIONS t,
         LATERAL FLATTEN(input => t."assignee_harmonized") f
    WHERE f.value:"name"::STRING ILIKE '%denso%'
      AND t."filing_date" > 0
),
citing_links AS (
    SELECT 
        t."publication_number"               AS "citing_pub"
    FROM PATENTS.PATENTS.PUBLICATIONS t,
         LATERAL FLATTEN(input => t."citation") g,
         denso_pubs d
    WHERE g.value:"publication_number"::STRING = d."publication_number"
),
citing_assignee AS (
    SELECT 
        cl."citing_pub",
        f.value:"name"::STRING AS "citing_assignee"
    FROM citing_links cl
    JOIN PATENTS.PATENTS.PUBLICATIONS p
      ON cl."citing_pub" = p."publication_number",
    LATERAL FLATTEN(input => p."assignee_harmonized") f
    WHERE f.value:"name"::STRING NOT ILIKE '%denso%'
),
citing_first_cpc AS (
    SELECT 
        t."publication_number"               AS "citing_pub",
        SUBSTR(c.value:"code"::STRING, 1, 4) AS "cpc4"
    FROM PATENTS.PATENTS.PUBLICATIONS t,
         LATERAL FLATTEN(input => t."cpc") c
    WHERE c.value:"first"::BOOLEAN = TRUE
)
