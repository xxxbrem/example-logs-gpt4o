```sql
--Description: Join the **first CPC subclass** of citing patents with its full title
--              from the CPC_DEFINITION table.
WITH citing_first_cpc AS (
    SELECT 
        t."publication_number"               AS "citing_pub",
        SUBSTR(c.value:"code"::STRING, 1, 4) AS "cpc4"
    FROM PATENTS.PATENTS.PUBLICATIONS t,
         LATERAL FLATTEN(input => t."cpc") c
    WHERE c.value:"first"::BOOLEAN = TRUE
)
SELECT 
    cf."citing_pub",
    cd."symbol",
    cd."titleFull"
FROM citing_first_cpc cf
JOIN PATENTS.PATENTS.CPC_DEFINITION cd
  ON cd."symbol" = cf."cpc4"
LIMIT 20;
