WITH sample_citing AS (
    SELECT DISTINCT
        t."publication_number"
    FROM PATENTS.PATENTS.PUBLICATIONS t,
         LATERAL FLATTEN(input => t."assignee_harmonized") f
    WHERE f.value:"name"::STRING ILIKE '%toyota%'    -- arbitrary non-Denso example
      AND t."citation" IS NOT NULL
    LIMIT 20
)
SELECT 
    s."publication_number",
    c.value:"code"::STRING        AS "cpc_code",
    c.value:"first"::BOOLEAN      AS "is_first"
FROM sample_citing s,
     PATENTS.PATENTS.PUBLICATIONS p,
     LATERAL FLATTEN(input => p."cpc") c
WHERE p."publication_number" = s."publication_number"
  AND c.value:"first"::BOOLEAN = TRUE
LIMIT 20;
