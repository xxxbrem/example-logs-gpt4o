  ON ca."citing_pub" = cfc."citing_pub"
JOIN PATENTS.PATENTS.CPC_DEFINITION cd
  ON cd."symbol" = cfc."cpc4"
GROUP BY 
    ca."citing_assignee",
    cd."titleFull"
LIMIT 20;
