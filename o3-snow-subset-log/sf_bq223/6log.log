    ca."citing_assignee",
    cd."titleFull"                     AS "cpc_subclass_title",
    COUNT(*)                           AS "citation_count"
FROM citing_assignee ca
JOIN citing_first_cpc cfc
  ON ca."citing_pub" = cfc."citing_pub"
JOIN PATENTS.PATENTS.CPC_DEFINITION cd
  ON cd."symbol" = cfc."cpc4"
GROUP BY 
    ca."citing_assignee",
    cd."titleFull"
LIMIT 20;
