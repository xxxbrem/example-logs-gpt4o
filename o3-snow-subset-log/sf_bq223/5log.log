```sql
--Description:
-- Identify non-DENSO assignees that cite patents assigned to DENSO CORP and list the
-- CPC subclass (first 4-digit code) titles associated with those citing patents.
-- • densopubs   : every publication_number whose assignee_harmonized contains “DENSO”.
-- • citingpubs  : distinct publications that cite at least one densopub and have a filing_date.
--                 This early filtering avoids flattening CPC for the entire corpus.
-- • Final step  : for each citing publication, take its primary CPC code (flag “first” = TRUE),
--                 join to CPC_DEFINITION for the full title, exclude any citing assignee whose
--                 name contains “DENSO”, then aggregate counts by (assignee × CPC title).

WITH "densopubs" AS (
    SELECT DISTINCT p."publication_number"
    FROM PATENTS.PATENTS.PUBLICATIONS p,
         LATERAL FLATTEN(input => p."assignee_harmonized") a
    WHERE a.value:"name"::STRING ILIKE '%DENSO%'          -- captures “DENSO”, “NIPPON DENSO”, etc.
), "citingpubs" AS (
    SELECT DISTINCT p2."publication_number"
    FROM PATENTS.PATENTS.PUBLICATIONS p2,
         LATERAL FLATTEN(input => p2."citation") cit
    WHERE cit.value:"publication_number"::STRING IN (SELECT "publication_number" FROM "densopubs")
      AND p2."filing_date" IS NOT NULL                    -- keep only records with valid filing date
)
SELECT
    ass.value:"name"::STRING                   AS "citing_assignee",
    cd."titleFull"                             AS "primary_cpc_title",
    COUNT(*)                                   AS "citation_cnt"
FROM PATENTS.PATENTS.PUBLICATIONS pub
     JOIN "citingpubs" cp
       ON pub."publication_number" = cp."publication_number"
     ,LATERAL FLATTEN(input => pub."assignee_harmonized") ass
     ,LATERAL FLATTEN(input => pub."cpc") cpc
     JOIN PATENTS.PATENTS.CPC_DEFINITION cd
       ON cd."symbol" = SUBSTR(cpc.value:"code"::STRING, 1, 4)   -- 4-digit CPC symbol
WHERE cpc.value:"first"::BOOLEAN = TRUE           -- primary CPC only
  AND ass.value:"name"::STRING NOT ILIKE '%DENSO%'-- exclude DENSO as a citer
GROUP BY
    ass.value:"name"::STRING,
    cd."titleFull"
ORDER BY
    "citation_cnt" DESC
LIMIT 20;
